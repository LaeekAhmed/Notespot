{
   "name": "notespot-backend",
   "version": "1.0.0",
   "description": "Open source notes sharing platform",
   "main": "dist/index.js",
   "type": "module",
   "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "tsx index.ts",
      "debug": "tsx --inspect=0.0.0.0:2000 index.ts",
      "devStart": "nodemon --exec \"tsx\" index.ts",
      "clean": "rm -rf dist",
      "type-check": "tsc --noEmit",
      "test-functions": "tsx test-functions.ts",
      "repl": "tsx repl.ts"
   },
   "author": "Laeek Ahmed Shaikh",
   "dependencies": {
      "@aws-sdk/client-s3": "^3.627.0",
      "@aws-sdk/client-sts": "^3.817.0",
      "@aws-sdk/lib-storage": "^3.627.0",
      "@aws-sdk/s3-request-presigner": "^3.830.0",
      "@clerk/backend": "^2.3.0",
      "@clerk/express": "^1.7.2",
      "alert": "^5.1.4",
      "axios": "^0.27.2",
      "blob": "^0.1.0",
      "body-parser": "^1.20.0",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "express": "^4.18.1",
      "express-openid-connect": "^2.16.0",
      "ioredis": "^5.2.3",
      "method-override": "^3.0.0",
      "mongoose": "^6.5.1",
      "multer": "^1.4.5-lts.1",
      "multer-s3": "^3.0.1",
      "multer-s3-v2": "^2.2.1",
      "node-blob": "^0.0.2",
      "pdfjs": "^2.4.7",
      "redis": "^4.0.0-rc.4",
      "serve": "^14.0.1",
      "util": "^0.12.4",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.5",
      "@types/compression": "^1.8.0",
      "@types/cors": "^2.8.18",
      "@types/express": "^5.0.2",
      "@types/method-override": "^3.0.0",
      "@types/multer": "^1.4.12",
      "@types/node": "^22.15.24",
      "@types/uuid": "^10.0.0",
      "dotenv": "^16.0.1",
      "nodemon": "^3.1.10",
      "ts-node": "^10.9.2",
      "tsx": "^4.19.4",
      "typescript": "^5.8.3"
   }
}